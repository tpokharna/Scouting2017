
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:fillViewport="true"
    android:layout_height="fill_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.trishlapokharna.steamfish2017.Teleop"
    android:id="@+id/scrollView">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:id="@+id/hello">

        #CHANGE SPY NAMES!!!!!

        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/nameRow"
            android:id="@+id/firstTableLayout">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/offenseTableRow">

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".15"
                    android:layout_height="wrap_content"
                    android:text="Offense or Defense?"
                    android:textAppearance="@android:style/TextAppearance.Medium"
                    android:id="@+id/offenseTextView" />

                <ToggleButton
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:layout_height="wrap_content"
                    android:textOn="Offense"
                    android:paddingBottom="20dp"
                    android:paddingTop="20dp"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textOff="Defense"
                    android:layout_marginRight="30dp"
                    android:id="@+id/offenseToggleButton"
                    android:checked="false" />
            </TableRow>
        </TableLayout>

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:layout_below="@+id/firstTableLayout"
            android:id = "@+id/gearTextTableRow1">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/gearTextRow1">


            <TextView
            android:layout_width="0dp"
            android:layout_weight=".4"
            android:layout_height="wrap_content"
            android:text="Gears"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:id="@+id/gearTextView1" />

            <TextView
                android:layout_width="0dp"
                android:layout_weight=".6"
                android:layout_height="wrap_content"
                android:text=""
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_marginLeft="20dp"
                android:id="@+id/gearView1"
                />

        </TableRow>
        </TableLayout>



        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_below="@+id/gearTextTableRow1"
            android:id = "@+id/highButtonTableRow">

            <Space
                android:layout_width="0dp"
                android:layout_weight=".11"
                android:layout_height="20px"
                tools:targetApi="ice_cream_sandwich" />

            <Button
                android:layout_width="0dp"
                android:layout_weight="0.26"
                android:layout_height="wrap_content"
                android:text="1"
                android:padding="20dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/gearHitButton" />

            <Button
                android:layout_width="0dp"
                android:layout_weight="0.26"
                android:layout_height="wrap_content"
                android:text="O"
                android:padding="20dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/gearMissButton" />

            <Button
                android:layout_width="0dp"
                android:layout_weight="0.26"
                android:layout_height="wrap_content"
                android:text="Back"
                android:padding="20dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/gearDeleteButton" />

            <Space
                android:layout_width="0dp"
                android:layout_weight=".11"
                android:layout_height="wrap_content"
                tools:targetApi="ice_cream_sandwich" />

        </TableRow>


        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/highButtonTableRow"
            android:id="@+id/cycleTimeTableRow">


            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/cycleTimeRow">

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".15"
                    android:layout_height="wrap_content"
                    android:text="Cycle Time"
                    android:textAppearance="@android:style/TextAppearance.Medium"
                    android:id="@+id/cycleTimeTextView" />

                <ToggleButton
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:layout_height="wrap_content"
                    android:textOn="Slow"
                    android:paddingBottom="20dp"
                    android:paddingTop="20dp"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textOff="Fast"
                    android:layout_marginRight="30dp"
                    android:id="@+id/cycleTimeToggleButton"
                    android:checked="false" />
            </TableRow>
        </TableLayout>

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:layout_below="@id/cycleTimeTableRow"
            android:id="@+id/highGoalLayout">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:layout_below="@+id/crossTableRow"
                android:id = "@+id/highGoalTextTableRow">

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".4"
                    android:layout_height="wrap_content"
                    android:text="High Goals"
                    android:textAppearance="@android:style/TextAppearance.Medium"
                    android:id="@+id/highGoalTextView1" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".6"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_marginLeft="20dp"
                    android:text=""
                    android:id="@+id/highGoalView"
                    />

            </TableRow>


            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_below="@+id/highTextTableRow"
                android:id = "@+id/highTableRow">

                <Space
                    android:layout_width="0dp"
                    android:layout_weight=".11"
                    android:layout_height="20px"
                    tools:targetApi="ice_cream_sandwich" />

                <Button
                    android:layout_width="0dp"
                    android:layout_weight="0.26"
                    android:layout_height="wrap_content"
                    android:text="5"
                    android:padding="20dp"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:id="@+id/highGoalAddFive" />

                <Button
                    android:layout_width="0dp"
                    android:layout_weight="0.26"
                    android:layout_height="wrap_content"
                    android:text="1O"
                    android:padding="20dp"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:id="@+id/highGoalAddTen" />

                <Button
                    android:layout_width="0dp"
                    android:layout_weight="0.26"
                    android:layout_height="wrap_content"
                    android:text="20"
                    android:padding="20dp"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:id="@+id/highGoalAddTwenty" />

                <Space
                    android:layout_width="0dp"
                    android:layout_weight=".11"
                    android:layout_height="wrap_content"
                    tools:targetApi="ice_cream_sandwich" />

            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:id="@+id/spinnerRow">

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".5"
                    android:layout_height="wrap_content"
                    android:textAppearance="@android:style/TextAppearance.Medium"
                    android:text="Consistency for High Goals"
                    android:id="@+id/textView13" />
                <Spinner
                    android:layout_width="0dp"
                    android:layout_weight=".5"
                    android:layout_height="wrap_content"
                    android:entries="@array/category_d"
                    android:id="@+id/spinnerD" />
            </TableRow>

        </TableLayout>

        #CHANGE SPY NAMES!!!!!

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:layout_below="@id/highGoalLayout"
            android:id="@+id/lowGoalLayout">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:layout_below="@+id/crossTableRow"
                android:id = "@+id/lowTextTableRow1">

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".4"
                    android:layout_height="wrap_content"
                    android:text="Low Goals"
                    android:textAppearance="@android:style/TextAppearance.Medium"
                    android:id="@+id/lowGoalTextView" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".6"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_marginLeft="20dp"
                    android:id="@+id/lowGoalView2"
                    />

            </TableRow>
        </TableLayout>


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_below="@+id/lowGoalLayout"
            android:id = "@+id/lowButtonTableRow">

            <Space
                android:layout_width="0dp"
                android:layout_weight=".11"
                android:layout_height="20px"
                tools:targetApi="ice_cream_sandwich" />

            <Button
                android:layout_width="0dp"
                android:layout_weight="0.26"
                android:layout_height="wrap_content"
                android:text="5"
                android:padding="20dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/lowGoalAddFiveButton" />

            <Button
                android:layout_width="0dp"
                android:layout_weight="0.26"
                android:layout_height="wrap_content"
                android:text="1O"
                android:padding="20dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/lowGoalAddTenButton" />

            <Button
                android:layout_width="0dp"
                android:layout_weight="0.26"
                android:layout_height="wrap_content"
                android:text="20"
                android:padding="20dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/lowGoalAddTwentyButton" />

            <Space
                android:layout_width="0dp"
                android:layout_weight=".11"
                android:layout_height="wrap_content"
                tools:targetApi="ice_cream_sandwich" />

        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_below="@+id/lowButtonTableRow"
            android:id = "@+id/consistencyRow">

            <TextView
                android:layout_width="0dp"
                android:layout_weight=".5"
                android:layout_height="wrap_content"
                android:textAppearance="@android:style/TextAppearance.Medium"
                android:text="Consistency for Low Goals"
                android:id="@+id/textView11" />
            <Spinner
                android:layout_width="0dp"
                android:layout_weight=".5"
                android:layout_height="wrap_content"
                android:entries="@array/category_b"
                android:id="@+id/spinnerB" />
        </TableRow>

        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/consistencyRow"
            android:id="@+id/takeoff">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/ropeTableRow">
                <ToggleButton
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:layout_height="wrap_content"
                    android:textOn="Climb"
                    android:paddingBottom="20dp"
                    android:paddingTop="20dp"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textOff="None"
                    android:layout_marginRight="30dp"
                    android:id="@+id/takeoffToggleButton"
                    android:checked="false" />
            </TableRow>
        </TableLayout>


    </RelativeLayout>
</ScrollView>